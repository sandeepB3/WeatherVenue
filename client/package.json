{
  "name": "weathervenue-client",
  "version": "1.0.0",
  "description": "Not a standalone client. It only generates client scripts and styles that are served publicly",
  "source": [
    "js/weathervenue.js"
  ],
  "targets": {
    "default": {
      "engines": {
        "browsers": "> 0.25%"
      },
      "distDir": "../public/js/",
      "publicUrl": ".",
      "sourceMap": true
    }
  },
  "type": "module",
  "scripts": {
    "build": "find ../public/js/ -type f -delete && parcel build && sass style:../public/css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@googlemaps/js-api-loader": "^1.14.3",
    "bootstrap": "^5.1.3",
    "detect-inapp": "^1.4.0",
    "dom-to-image": "^2.6.0",
    "svg-injector": "^1.1.3"
  },
  "devDependencies": {
    "@types/google.maps": "^3.49.2",
    "@types/web": "0.0.68",
    "buffer": "^6.0.3",
    "parcel": "^2.6.2",
    "process": "^0.11.10"
  },
  "keywords": [
    "javascript",
    "google maps",
    "vanilla js"
  ],
  "author": "bacloud22",
  "license": "ISC"
}
